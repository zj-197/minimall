/* pages/detail/childcpn/skus/skus.wxss */
.desc {
  padding: 0rpx 10rpx 30rpx;
  color: #777;
  border-bottom: 12rpx solid #f2f2f2;
}
.dot{
  width: 220rpx;
  height: 3rpx;
  background: black;
  margin: 40rpx 0;
  position: relative;
}
.dot::before {
  content: '';
  width: 14rpx;
  height: 14rpx;
  background: black;
  position: absolute;
  bottom: 0rpx;
}
.rightdot::after{
  content: '';
  width: 14rpx;
  height: 14rpx;
  background: black;
  position: absolute;
  bottom: 0rpx;
}
.rightdot {
  width: 220rpx;
  height: 3rpx;
  background: black;
  margin: 40rpx 0 20rpx;
  position: relative;
  float: right;
}
.clear {
  clear: both;
}
